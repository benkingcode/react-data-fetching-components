{
  "name": "react-data-fetching-components",
  "version": "0.1.0",
  "description": "Asynchronously load data for your React components with SSR",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --watch",
    "build": "webpack",
    "prepublish": "webpack"
  },
  "author": "Ben King <ben@designbybenking.co.uk>",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current",
            "uglify": true
          },
          "exclude": ["transform-regenerator"],
          "useBuiltIns": true
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-react-jsx",
      "transform-class-properties"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbbk/react-data-fetching-components.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "fetch",
    "data",
    "ssr",
    "ajax",
    "getInitialData",
    "getInitialProps"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dbbk/react-data-fetching-components/issues"
  },
  "homepage": "https://github.com/dbbk/react-data-fetching-components#readme",
  "dependencies": {
    "object-hash": "^1.2.0",
    "prop-types": "^15.6.0",
    "react-tree-walker": "^2.1.3"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "webpack": "^3.10.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint-plugin-class-property": "^1.1.0"
  }
}
